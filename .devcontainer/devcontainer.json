{
  "name": "health care (cpu)",
  "build": { "dockerfile": "../Dockerfile", "context": ".." },
  "remoteUser": "appuser",
  "workspaceFolder": "/app",
  "mounts": [
    "source=${localWorkspaceFolder},target=/app,type=bind"
  ],
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-toolsai.jupyter",
        "ms-azuretools.vscode-docker"
      ]
    }
  },
  "postCreateCommand": "python -m pip install -r requirements.txt || true"
}